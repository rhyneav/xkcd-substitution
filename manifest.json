{
    "manifest_version": 2,

    "name": "Substitutions",
    "short_name": "Make reading the news great again",
    "description": "Replaces words found from XKCD's comic to make reading the news more fun.",
    "version": "1.0",
    
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            
            "js": ["script.js"],
            
            "run_at": "document_end"
        }
    ]

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}